            "\u9664\u975e\u60a8\u8c03\u6574\u5b58\u6b3e\u9650\u989d\u6216\u5220\u9664\u5b58\u6b3e\u9650\u989d\uff0c\u5426\u5219\u60a8\u8bbe\u7f6e\u7684\u5b58\u6b3e\u9650\u989d\u5c06\u81ea\u52a8\u7eed\u8ba2",
            "\u589e\u52a0\u548c\u51cf\u5c11\u60a8\u7684\u4efb\u4f55\u5b58\u6b3e\u9650\u5236\uff1a\u5982\u679c\u60a8\u964d\u4f4e\u8bbe\u7f6e\u7684\u4efb\u4f55\u5b58\u6b3e\u9650\u5236\uff0c\u5219\u5c06\u7acb\u5373\u5e94\u7528\u8fd9\u79cd\u51cf\u5c11\u3002\u5982\u679c\u60a8\u589e\u52a0\u4e86\u8bbe\u5b9a\u7684\u4efb\u4f55\u5b58\u6b3e\u9650\u989d\uff0c\u5219\u8fd9\u79cd\u589e\u52a0\u5c06\u572824\u5c0f\u65f6\u5185\u751f\u6548\u3002",
            "\u5f53\u60a8\u8fbe\u5230\u8bbe\u7f6e\u7684\u5b58\u6b3e\u9650\u5236\u65f6\uff0c\u5c06\u4f1a\u51fa\u73b0\u5f39\u51fa\u901a\u77e5\u3002\u4e00\u65e6\u8fbe\u5230\u8bbe\u5b9a\u7684\u5b58\u6b3e\u9650\u989d\uff0c\u60a8\u5c31\u4e0d\u53ef\u80fd\u5c06\u4efb\u4f55\u8fdb\u4e00\u6b65\u7684\u5b58\u6b3e\u5b58\u5165\u60a8\u7684\u5e10\u6237\u3002",
            "2.\u8d4c\u6ce8\u9650\u5236 - \u60a8\u53ef\u4ee5\u8bbe\u7f6e\u60a8\u53ef\u4ee5\u5728\u4ee5\u4e0b\u9636\u6bb5\uff08\u6216\u4ee5\u4e0b\u65f6\u671f\u7ec4\u5408\uff09\u4e0b\u6ce8\u7684\u6700\u5927\u91d1\u989d\u7684\u9650\u5236\uff1a",
            [
                "\u65e5\u5386\u65e5",
                "\u65e5\u5386\u5468",
                "\u65e5\u5386\u6708"
            ],
            "\u76f8\u5173\u9650\u5236\u7684\u65f6\u95f4\u6bb5",
            "\u65e5\u5386\u65e5\u8d4c\u6ce8\u9650\u5236\uff1a\u65e5\u5386\u65e5\u8d4c\u6ce8\u9650\u5236\u4ece\u65e5\u5386\u65e5\u7684\u51cc\u666812\u70b9\u5f00\u59cb\uff0c\u8be5\u65e5\u5386\u65e5\u751f\u6548\uff0c\u5e76\u5728\u540c\u4e00\u65e5\u5386\u65e5\u665a\u4e0a11:59\u7ed3\u675f\u3002",
            "\u65e5\u5386\u5468\u7684\u8d4c\u6ce8\u9650\u5236\uff1a\u6bcf\u5468\u7684\u4e0b\u6ce8\u9650\u5236\u4ece\u65e5\u5386\u5468\u7684\u661f\u671f\u4e00\u51cc\u666812\u70b9\u5f00\u59cb\uff0c\u8be5\u8d4c\u6ce8\u9650\u5236\u751f\u6548\u5e76\u5728\u540c\u4e00\u65e5\u5386\u5468\u7684\u661f\u671f\u65e5\u7ed3\u675f",
            "\u65e5\u5386\u6708\u8d4c\u6ce8\u9650\u5236\uff1a\u6bcf\u6708\u4e0b\u6ce8\u9650\u5236\u4ece\u65e5\u5386\u6708\u7684\u7b2c\u4e00\u4e2a\u65e5\u5386\u65e5\u51cc\u666812\u70b9\u5f00\u59cb\uff0c\u8be5\u65e5\u5386\u6708\u751f\u6548\uff0c\u5e76\u5728\u65e5\u5386\u6708\u7684\u6700\u540e\u4e00\u5929\u7ed3\u675f\uff0c\u5728\u65e5\u5386\u6708\u7684\u6700\u540e\u4e00\u5929\u7ed3\u675f\u3002",
            "\u4e3a\u4e86\u8bbe\u7f6e\u8d4c\u6ce8\u9650\u5236\u6216\u8c03\u6574\u8d4c\u6ce8\u9650\u5236\uff0c\u4ece\u201c\u6211\u7684\u5e10\u6237\u201d\u9875\u9762>\u5355\u51fb\u8d1f\u8d23\u4efb\u7684\u6e38\u620f\u9009\u9879\u5361>\u5355\u51fb\u4e0b\u6ce8\u9650\u5236>\u8bbe\u7f6e/\u8c03\u6574\u9002\u7528\u7684\u8d4c\u6ce8\u9650\u5236",
            "\u9664\u975e\u60a8\u8c03\u6574\u8d4c\u6ce8\u9650\u5236\u6216\u5220\u9664\u8d4c\u6ce8\u9650\u5236\uff0c\u5426\u5219\u60a8\u8bbe\u7f6e\u7684\u8d4c\u6ce8\u9650\u5236\u5c06\u81ea\u52a8\u7eed\u8ba2",
            "\u589e\u52a0\u548c\u51cf\u5c11\u4efb\u4f55\u8d4c\u6ce8\u9650\u5236\uff1a\u5982\u679c\u60a8\u964d\u4f4e\u8bbe\u7f6e\u7684\u4efb\u4f55\u8d4c\u6ce8\u9650\u5236\uff0c\u4e0b\u6b21\u60a8\u767b\u5f55\u5e10\u6237\u65f6\u5c06\u5e94\u7528\u6b64\u7c7b\u51cf\u5c11\u3002\u5982\u679c\u60a8\u589e\u52a0\u4e86\u8bbe\u5b9a\u7684\u4efb\u4f55\u8d4c\u6ce8\u9650\u5236\uff0c\u5219\u8fd9\u79cd\u589e\u52a0\u5c06\u572824\u5c0f\u65f6\u5185\u751f\u6548\u3002",
            "\u5f53\u60a8\u8fbe\u5230\u8bbe\u7f6e\u7684\u8d4c\u6ce8\u9650\u5236\u65f6\uff0c\u5c06\u51fa\u73b0\u5f39\u51fa\u901a\u77e5\u3002\u4e00\u65e6\u8fbe\u5230\u4e86\u8bbe\u5b9a\u7684\u8d4c\u6ce8\u9650\u5236\uff0c\u60a8\u5c31\u4e0d\u53ef\u80fd\u901a\u8fc7\u5e10\u6237\u8fdb\u4e00\u6b65\u8d4c\u535a",
            "\u5982\u679c\u60a8\u8981\u6c42\u5728\u76f8\u5173\u7684\u8d4c\u6ce8\u9650\u5236\u671f\u95f4\u591a\u6b21\u589e\u52a0\u4efb\u4f55\u8d4c\u6ce8\u9650\u5236\uff0c\u5219\u63d0\u9ad8\u8d4c\u6ce8\u9650\u5236\u7684\u6700\u65b0\u8bf7\u6c42\u5c06\u572824\u5c0f\u65f6\u5185\u8fdb\u5165\u3002",
            "3.\u4e0a\u884c\u65f6\u95f4\u9650\u5236 - \u60a8\u53ef\u4ee5\u8bbe\u7f6e\u6bcf\u4e2a\u65e5\u5386\u65e5\u5728\u7f51\u7ad9\u4e0a\u82b1\u8d39\u7684\u65f6\u95f4\u7684\u9650\u5236\u3002",
            "\u4e3a\u4e86\u8bbe\u7f6e\u6e38\u620f\u65f6\u95f4\u9650\u5236\u6216\u8c03\u6574\u6e38\u620f\u65f6\u95f4\u9650\u5236\uff1a\u4ece\u201c\u6211\u7684\u5e10\u6237\u201d\u9875\u9762>\u5355\u51fb\u8d1f\u8d23\u4efb\u7684\u6e38\u620f\u9009\u9879\u5361>\u5355\u51fb\u201c\u4e0a\u884c\u65f6\u95f4\u9650\u5236\u201d>\u63d2\u5165/\u8c03\u6574\u6e38\u620f\u65f6\u95f4\u9650\u5236>\u5355\u51fb\u201c\u66f4\u65b0\u9650\u5236\u201d\u3002",
            "\u9664\u975e\u60a8\u8c03\u6574\u6bd4\u8d5b\u65f6\u95f4\u9650\u5236\u6216\u5220\u9664\u4e0a\u573a\u65f6\u95f4\u9650\u5236\uff0c\u5426\u5219\u60a8\u8bbe\u7f6e\u7684\u6e38\u620f\u65f6\u95f4\u9650\u5236\u5c06\u81ea\u52a8\u7eed\u8ba2\u3002",
            "\u4e0a\u573a\u65f6\u95f4\u9650\u5236\u7684\u521d\u59cb\u8bbe\u7f6e\uff1a\u5f53\u60a8\u9996\u6b21\u8bbe\u7f6e\u6bd4\u8d5b\u65f6\u95f4\u65f6\uff0c\u8fd9\u79cd\u4e0a\u573a\u65f6\u95f4\u9650\u5236\u5c06\u5728\u65e5\u5386\u65e5\u751f\u6548",
            "\u589e\u52a0\u548c\u51cf\u5c11\u4e0a\u573a\u65f6\u95f4\u9650\u5236\uff1a\u5982\u679c\u60a8\u964d\u4f4e\u8bbe\u7f6e\u7684\u4e0a\u9650\u65f6\u95f4\u9650\u5236\uff0c\u4e0b\u6b21\u767b\u5f55\u5e10\u6237\u65f6\uff0c\u8fd9\u79cd\u51cf\u5c11\u5c06\u751f\u6548\u3002\u5982\u679c\u60a8\u589e\u52a0\u4e86\u8bbe\u5b9a\u7684\u4e0a\u9650\u65f6\u95f4\u9650\u5236\uff0c\u90a3\u4e48\u4e0b\u4e00\u6b21\u5728\u65e5\u5386\u65e5\u7684\u65e5\u5386\u65e5\u767b\u5f55\u5230\u60a8\u7684\u5e10\u6237\u65f6\uff0c\u5c06\u4f1a\u589e\u52a0\u6b64\u7c7b\u589e\u52a0",
            "\u5f53\u60a8\u8fbe\u5230\u8bbe\u7f6e\u7684\u6e38\u620f\u65f6\u95f4\u9650\u5236\u65f6\uff0c\u5c06\u4f1a\u51fa\u73b0\u5f39\u51fa\u901a\u77e5\uff0c\u5e76\u4e14\u60a8\u5c06\u81ea\u52a8\u767b\u5f55\u5e10\u6237\u3002",
            "\u81ea\u6211\u6392\u65a5",
            [
                "\u4e00\u5e74",
                "5\u5e74",
                "\u5bff\u547d"
            ],
            "\u4e3a\u4e86\u901a\u8fc7\u7f51\u7ad9\u8fdb\u884c\u81ea\u6211\u9694\u79bb\uff0c\u4ece\u201c\u6211\u7684\u5e10\u6237\u201d\u9875\u9762>\u5355\u51fb\u8d1f\u8d23\u4efb\u7684\u6e38\u620f\u9009\u9879\u5361>\u5355\u51fb\u201c\u8bf7\u6c42\u81ea\u6211\u6392\u9664\u201d>\u8fd9\u5c06\u5bfc\u81f4\u6536\u94f6\u5458\u5728\u5176\u4e2d\u60a8\u53ef\u4ee5\u9009\u62e9\u8981\u81ea\u6211\u5212\u5206\u7684\u65f6\u95f4>\u7136\u540e\u5355\u51fb\u63d0\u4ea4\u3002\u7136\u540e\uff0c\u60a8\u5c06\u4ece\u7f51\u7ad9\u4e0a\u767b\u5f55\u3002",
            "\u4e00\u65e6\u60a8\u81ea\u6211\u6392\u65a5\uff0c\u60a8\u5c31\u4e0d\u4f1a\u5c06\u4efb\u4f55\u65b0\u7684\u8d4c\u6ce8\u653e\u5728\u7f51\u7ad9\u4e0a\u3002\u4f46\u662f\uff0c\u5728\u60a8\u8bbe\u5b9a\u7684\u81ea\u884c\u6392\u9664\u671f\u5185\uff0c\u60a8\u53ef\u4ee5\u64a4\u56de\u5269\u4f59\u7684\u8d44\u91d1\uff0c\u4f46\u524d\u63d0",
            "\u8bf7\u6ce8\u610f\uff0c\u5982\u679c\u60a8\u4ece\u7f51\u7ad9\u4e0a\u81ea\u884c\u5265\u593a\u4e86\u4e00\u751f\uff0c\u5219\u53ef\u4ee5\u8981\u6c42\u5728\u81f3\u5c11\u4e00\u5e74\u4e4b\u540e\u64a4\u9500\u751f\u547d\u5468\u671f\u7684\u81ea\u6211\u5265\u593a\uff0c\u4ece\u4e00\u5e74\u6765\u7684\u81ea\u6211\u6392\u65a5\u5f00\u59cb\uff0c\u901a\u8fc7\u8054\u7cfbsupport@niblsinc771.onmicrosoft\u3002com\u3002",
            "\u4e00\u65e6\u60a8\u901a\u8fc7\u7f51\u7ad9\u8fdb\u884c\u81ea\u6211\u6392\u65a5\uff0c\u6211\u4eec\u5c06\uff1a",
            [
                "\u963b\u6b62\u60a8\u7684\u5e10\u6237\u3002",
                "\u5f53\u6211\u4eec\u68c0\u6d4b\u5230\u7f51\u7ad9\u65f6\uff0c\u8bf7\u963b\u6b62\u60a8\u5c1d\u8bd5\u4f7f\u7528\u8be5\u7f51\u7ad9\u6253\u5f00\u7684\u4efb\u4f55\u65b0\u5e10\u6237"
            ],
            "\u5728\u60a8\u7684\u81ea\u6211\u6392\u65a5\u671f\u95f4\uff0c\u6211\u4eec\u5c06\u91c7\u53d6\u6240\u6709\u5408\u7406\u7684\u63aa\u65bd\uff0c\u4ee5\u786e\u4fdd\u60a8\u4e0d\u4f1a\u6536\u5230\u6211\u4eec\u7684\u4efb\u4f55\u4fc3\u9500\u6750\u6599\u3002",
            "\u63a8\u9001\u901a\u77e5",
            "\u5982\u679c\u60a8\u5c06\u6211\u4eec\u7684\u4efb\u4f55\u5e94\u7528\u7a0b\u5e8f\u4e0b\u8f7d\u5230\u60a8\u7684\u8bbe\u5907\u6216\u76f4\u63a5\u4ece\u7f51\u7ad9\u4e0a\u4e0b\u8f7d\u5230\u8bbe\u5907\uff0c\u5e76\u4e14\u540c\u610f\u63a5\u6536\u63a8\u9001\u901a\u77e5\uff0c\u5982\u679c\u60a8\u5e0c\u671b\u505c\u6b62\u53d1\u9001\u7ed9\u60a8\u7684\u901a\u77e5\uff0c\u5219\u5fc5\u987b\u901a\u8fc7\u76f8\u5173\u8bbe\u5907\u6216\u5220\u9664\u6765\u6267\u884c\u6b64\u64cd\u4f5c\u3002\u5e94\u7528\u7a0b\u5e8f\u3002\u4f46\u662f\uff0c\u5982\u679c\u60a8\u5728\u7f51\u7ad9\u4e0a\u81ea\u6211\u9694\u79bb\uff0c\u6211\u4eec\u5c06\u505c\u6b62\u60a8\u6536\u5230\u63a8\u9001\u901a\u77e5"
        ]
    },
